<?xml version='1.0' encoding='utf-8'?>
<widget id="org.fishywiki.testapp" version="1.0.0" versionCode="10" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <content src="index.html" />
    <access origin="*" />
    <preference name="phonegap-version" value="3.3.0" />
    <preference name="SplashScreen" value="splash" />
    <name>Brendan\'s sample Config.xml</name>
    <name>My Test App</name>
    <description>
        An example config.xml for online PhoneGap builds.
   </description>
    <author email="someone@somecompany.com" href="http://fishywiki.org">
        Author Name
   </author>
    <icon src="icon.png" />
    <gap:plugin name="org.apache.cordova.camera" />
    <gap:plugin name="nl.x-services.plugins.socialsharing" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
</widget>
